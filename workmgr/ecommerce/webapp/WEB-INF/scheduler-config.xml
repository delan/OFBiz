<?xml version="1.0" encoding="UTF-8" ?>

<!-- example scheduled job: -->
<scheduler>
  <schedule name="MainTestJob">
   	<duration start="2001-10-01 00:00" end="2001-10-01 23:59"/>
   	<interval type="minute" value="1" repeat="true"/>  
    <event type="java" path="org.ofbiz.core.event.SchedulerTestEvent" invoke="testJob"/>    
    <parameter name="parmName" value="parmValue"/>
  </schedule>
</scheduler>
