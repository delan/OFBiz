<?xml version="1.0"?>

<page xmlns="http://www.einnovation.com/xmlns/WSP/Admin/Content"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<title>Copy a Page</title>
	<content>
		<dialog>
			<title>Copy Successful</title>
			<content>
				<border>
					The page $request.getParameter( 'sourcePath' ) has been
					copied to $request.getParameter( 'destinationPath' ).
				</border>
			</content>
			<buttons>
				<button name="View Copy" href="$url_util.xmlEscape( $request.getParameter( 'destinationPath' ) )"/>
			</buttons>
		</dialog>
	</content>
</page>
