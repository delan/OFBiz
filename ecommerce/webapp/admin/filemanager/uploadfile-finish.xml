<?xml version="1.0"?>

<page xmlns="http://www.einnovation.com/xmlns/WSP/Admin/Content"
    xmlns:html="http://www.w3.org/1999/xhtml" onload="window.focus()">
  <title>Upload Successful</title>
  <content>
  	#set( $editingPath = $request.getParameter( 'editingPath' ) )
    <dialog fill="yes">
      <title>Upload Successful</title>
      <content>
      	<box>
      		Your file was uploaded successfully.
      	</box>
      </content>
      <buttons>
        <submit-button name="Upload Another" href="/admin/filemanager/uploadfile.html?editingPath=$url_util.encode( $editingPath )"/>
        <cancel onclick="window.close()"/>
      </buttons>
    </dialog>
  </content>
</page>
