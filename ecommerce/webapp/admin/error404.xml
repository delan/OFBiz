<?xml version="1.0"?>

<page xmlns="http://www.einnovation.com/xmlns/WSP/Admin/Content"    xmlns:html="http://www.w3.org/1999/xhtml">
  <title>404 Error - file not found</title>
  <content>
 	<dialog>
 	<content>
 	<border>
 		<p>
 			#set( $fileError = $request.getAttribute( "wsp-exception" ) )
 			#set( $missingPath = $fileError.getPathWithError() )
      		Address <b>$missingPath</b> does not exist..
        </p>
      	<p>Would you like to add this file? <br/></p>
            <html:input type="hidden" name="path" value='$missingPath' />
     </border>
     </content>
     <buttons>
        <next href="/admin/filemanager/addpage/addpage2.html"/>
        <cancel href="javascript:window.back()"/>
      </buttons>
     </dialog>
 </content>
</page>
