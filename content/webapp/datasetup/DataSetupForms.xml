<forms>
    <!-- DataResourceType forms -->
    <form default-title-style="tableheadtext"
        default-tooltip-style="tabletext" default-widget-style="inputBox"
        name="AddDataResourceType" target="addDataResourceType" title="" type="single">
        <auto-fields-service service-name="createDataResourceType"/>
        <field name="submitButton" title="Add" widget-style="standardSubmit">
            <submit button-type="button"/>
        </field>
    </form>
    <form default-title-style="tableheadtext"
        default-tooltip-style="tabletext" default-widget-style="inputBox"
        list-name="contentList" name="UpdateDataResourceType"
        target="updateDataResourceType" title="" type="list">
        <auto-fields-service service-name="updateDataResourceType"/>
        <field name="dataResourceTypeId" title="DataResourceTypeId" widget-style="buttontext">
            <display/>
        </field>
        <field name="submitButton" title="Update" widget-style="standardSubmit">
            <submit button-type="button"/>
        </field>
        <field name="deleteLink" title="&amp;nbsp;" widget-style="buttontext">
            <hyperlink also-hidden="false" description="[Delete]" target="removeDataResourceType?dataResourceTypeId=${dataResourceTypeId}"/>
        </field>
    </form>
    
    <!-- DataResourceTypeAttr forms -->
    <form default-title-style="tableheadtext"
        default-tooltip-style="tabletext" default-widget-style="inputBox"
        name="AddDataResourceTypeAttr" target="addDataResourceTypeAttr"
        title="" type="single">
        <auto-fields-service service-name="createDataResourceTypeAttr"/>
        <field entity-name="DataResource" name="dataResourceTypeId">
            <drop-down allow-empty="false">
                <entity-options description="${description}"
                    entity-name="DataResourceType" key-field-name="dataResourceTypeId"/>
            </drop-down>
        </field>
        <field name="submitButton" title="Add" widget-style="standardSubmit">
            <submit button-type="button"/>
        </field>
    </form>
    <form default-title-style="tableheadtext"
        default-tooltip-style="tabletext" default-widget-style="inputBox"
        list-name="contentList" name="UpdateDataResourceTypeAttr"
        target="updateDataResourceTypeAttr" title="" type="list">
        <auto-fields-service service-name="createDataResourceTypeAttr"/>
        <field name="dataResourceTypeId" title="DataResourceTypeId" widget-style="buttontext">
            <display/>
        </field>
        <field name="attrName" title="Attribute Name" widget-style="buttontext">
            <display/>
        </field>
        <field name="deleteLink" title="&amp;nbsp;" widget-style="buttontext">
            <hyperlink also-hidden="false" description="[Delete]" target="removeDataResourceTypeAttr?dataResourceTypeId=${dataResourceTypeId}&amp;attrName=${attrName}"/>
        </field>
    </form>
    
