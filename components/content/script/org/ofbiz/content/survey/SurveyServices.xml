    <!-- Survey Response Methods -->
    <simple-method method-name="createSurveyResponse" short-description="Create Survey Response" login-required="false">
        <!-- lookup the survey -->
        <make-value value-name="surveyLookup" entity-name="Survey"/>
        <set-pk-fields value-name="surveyLookup" map-name="parameters"/>
        <find-by-primary-key map-name="surveyLookup" value-name="survey"/>
        <if-empty field-name="survey">
            <add-error><fail-message message="No survey found for survey ID: ${surveyLookup.surveyId}"/></add-error>
            <check-errors/>
        </if-empty>

        <!-- look up the applied questions -->
        <field-to-field field-name="surveyId" map-name="survey" to-map-name="questionApplLookup"/>
        <find-by-and list-name="questions" entity-name="SurveyQuestionAndAppl" map-name="questionApplLookup"/>
        <if-empty field-name="questions">
            <add-error><fail-message message="No questions found for survey ID: ${survey.surveyId}"/></add-error>
            <check-errors/>
        </if-empty>

        <!-- check if anonymous is allowed and if not that we have a partyId -->
        <if-compare field-name="isAnonymous" map-name="survey" operator="not-equals" value="Y">
            <if-empty field-name="partyId" map-name="parameters">
                <add-error><fail-message message="Anonymous response to this survey is not allowed"/></add-error>
                <check-errors/>
            </if-empty>
        </if-compare>

        <!-- if this is a single use and we have a partyId check for existing responses -->
        <if-compare field-name="allowMultiple" map-name="survey" operator="not-equals" value="Y">
            <if-not-empty field-name="partyId" map-name="parameters">
                <field-to-field field-name="partyId" map-name="parameters" to-map-name="responseLookup"/>
                <field-to-field field-name="surveyId" map-name="survey" to-map-name="responseLookup"/>
                <find-by-and list-name="existingResponses" entity-name="SurveyResponse" map-name="responseLookup"/>
                <if-not-empty field-name="existingResponses">
                    <add-error><fail-message message="You have already responded to this survey; multiple responses are not allowed"/></add-error>
                    <check-errors/>
                </if-not-empty>
            </if-not-empty>
        </if-compare>

        <!-- make sure we have answers to check -->
        <if-empty field-name="answers" map-name="parameters">
            <add-error><fail-message message="No answers were found"/></add-error>
            <check-errors/>
        </if-empty>
        <field-to-env field-name="answers" map-name="parameters" env-name="answers"/>

        <!-- make sure all required fields were responded to -->
        <iterate entry-name="question" list-name="questions">
            <if-compare field-name="requiredField" map-name="question" operator="equals" value="Y">
                <if-empty field-name="${question.surveyQuestionId}" map-name="answers">
                    <add-error><fail-message message="${question.question} requires a response"/></add-error>
                </if-empty>
            </if-compare>
            <!-- validate the field -->
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="CREDIT_CARD">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isCreditCard">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid credit-card number"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="GIFT_CARD">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isGiftCard">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid gift-card number"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="DATE">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isDate">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid date string (mm/dd/yyyy)"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="EMAIL">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isEmail">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid email address"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="NUMBER_CURRENCY">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isSignedDouble">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid currency amount"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="NUMBER_FLOAT">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isSignedDouble">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid decimal number"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="NUMBER_LONG">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isSignedLong">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid whole number"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
            <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="URL">
                <if-validate-method field-name="${question.surveyQuestionId}" map-name="answers" method="isUrl">
                <else>
                    <add-error><fail-message message="${question.question} requires a valid URL"></fail-message></add-error>
                </else>
                </if-validate-method>
            </if-compare>
        </iterate>
        <check-errors/>

        <now-timestamp-to-env env-name="nowTimestamp"/>

        <!-- create/update the response -->
        <if-compare field-name="allowUpdate" map-name="survey" operator="equals" value="Y">
          <if-not-empty field-name="surveyResponseId" map-name="parameters">
              <field-to-field field-name="surveyResponseId" map-name="parameters" to-map-name="lookupRespMap"/>
              <find-by-primary-key entity-name="SurveyResponse" map-name="lookupRespMap" value-name="surveyResponse"/>
          </if-not-empty>
        </if-compare>

        <if-empty field-name="surveyResponse">
            <make-value value-name="surveyResponse" entity-name="SurveyResponse"/>
            <sequenced-id-to-env sequence-name="SurveyResponse" env-name="surveyResponseId"/>
            <env-to-field env-name="surveyResponseId" map-name="surveyResponse"/>
            <set-nonpk-fields value-name="surveyResponse" map-name="parameters"/>
            <create-value value-name="surveyResponse"/>
        </if-empty>

        <if-empty field-name="responseDate" map-name="surveyResponse">
            <env-to-field env-name="nowTimestamp" field-name="responseDate" map-name="surveyResponse"/>
        </if-empty>
        <env-to-field env-name="nowTimestamp" field-name="lastModifiedDate" map-name="surveyResponse"/>

        <store-value value-name="surveyResponse"/>
        <field-to-result field-name="surveyResponseId" map-name="surveyResponse" result-name="surveyResponseId"/>

        <!-- lookup any existing response answers -->
        <field-to-field field-name="surveyResponseId" map-name="surveyResponse" to-map-name="lookupRespAns"/>
        <find-by-and list-name="existingAnswers" entity-name="SurveyResponseAnswer" map-name="lookupRespAns"/>
        <iterate entry-name="existingAnswer" list-name="existingAnswers">
            <field-to-field field-name="existingAnswer" to-field-name="${existingAnswer.surveyQuestionId}" to-map-name="currentAnswers"/>
        </iterate>

        <!-- create the response answers -->
        <iterate entry-name="question" list-name="questions">
            <if-not-empty field-name="currentAnswers">
                <field-to-env field-name="${question.surveyQuestionId}" map-name="currentAnswers" env-name="responseAnswer"/>
            </if-not-empty>

            <if-compare-field field-name="surveyQuestionId" map-name="responseAnswer"  operator="not-equals" to-field-name="surveyQuestionId" to-map-name="question">
                <make-value value-name="responseAnswer" entity-name="SurveyResponseAnswer"/>
                <field-to-field field-name="surveyResponseId" map-name="surveyResponse" to-map-name="responseAnswer"/>
                <field-to-field field-name="surveyQuestionId" map-name="question" to-map-name="responseAnswer"/>
                <create-value value-name="responseAnswer"/>
            </if-compare-field>

            <if-not-empty field-name="${question.surveyQuestionId}" map-name="answers">
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="BOOLEAN">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="booleanResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="EMAIL">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="DATE">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="URL">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="CREDIT_CARD">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="GIFT_CARD">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="PASSWORD">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="TEXT_SHORT">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="TEXT_LONG">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="TEXTAREA">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="textResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="NUMBER_CURRENCY">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="currencyResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="NUMBER_FLOAT">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="floatResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="NUMBER_LONG">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="numericResponse" to-map-name="responseAnswer"/>
                </if-compare>
                <if-compare field-name="surveyQuestionTypeId" map-name="question" operator="equals" value="OPTION">
                    <field-to-field field-name="${question.surveyQuestionId}" map-name="answers" to-field-name="surveyOptionSeqId" to-map-name="responseAnswer"/>
                </if-compare>

                <env-to-field env-name="nowTimestamp" field-name="answeredDate" map-name="responseAnswer"/>
                <store-value value-name="responseAnswer"/>
            </if-not-empty>
        </iterate>

        <!-- run the survey response service (async) -->
        <field-to-field field-name="surveyResponseId" map-name="surveyResponse" to-map-name="respServiceCtx"/>
        <if-not-empty field-name="responseService" map-name="survey">
            <call-service-asynch service-name="${survey.responseService}" in-map-name="respServiceCtx" include-user-login="true"/>  
        </if-not-empty> 
    </simple-method>
</simple-methods>
