<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE JposEntries PUBLIC "-//JavaPOS//DTD//EN" "jpos/res/jcl.dtd">

<JposEntries>
    <JposEntry logicalName="KeyboardScanner">
        <creation factoryClass="org.ofbiz.pos.jpos.factory.KybServiceInstanceFactory" serviceClass="org.ofbiz.pos.jpos.service.ScannerKybService"/>
        <vendor name="The Open For Business Project" url="http://www.ofbiz.org"/>
        <jpos category="Scanner" version="1.8"/>
        <product description="Generic Keyboard Wedge Scanner" name="KybScanner" url="www.ofbiz.org"/>
