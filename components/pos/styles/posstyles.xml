<?xml version="1.0" encoding="UTF-8"?>
<styles>
    <!-- XUI XML styles (related directly to those found in the screen definitions -->
    <style name="input">
        <color_back value="cccccc"/>
        <color_fore value="000000"/>
        <font_face value="verdana"/>
        <font_size value="22"/>
        <font_weight value="1"/>
        <font_italic value="0"/>
    </style>
    <style name="error">
        <color_back value="cc0033"/>
        <color_fore value="ffffff"/>
        <font_face value="verdana"/>
        <font_size value="22"/>
        <font_weight value="1"/>
        <font_italic value="0"/>
    </style>
    <style name="blackpanel">
        <color_back value="000000"/>
        <color_fore value="000000"/>
        <font_face value="verdana"/>
        <font_size value="10"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
    </style>
    <style name="errorpanel">
        <color_back value="cc0033"/>
        <color_fore value="ffffff"/>
        <font_face value="verdana"/>
        <font_size value="22"/>
        <font_weight value="1"/>
        <font_italic value="0"/>
    </style>
    <style name="panel">
        <color_back value="cccccc"/>
        <color_fore value="000000"/>
        <font_face value="verdana"/>
        <font_size value="10"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
    </style>
    <style name="journal">
        <color_back value="cccccc"/>
        <color_fore value="0000ff"/>
        <font_face value="verdana"/>
        <font_size value="10"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
    </style>
    <style name="menu">
        <color_back value="cccccc"/>
        <color_fore value="000000"/>
        <font_face value="verdana"/>
        <font_size value="12"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
    </style>

    <!-- embedded POS [menu] button styles (required but can be customized) -->
    <style name="numButton">
        <color_back value="cccccc"/>
        <color_fore value="000000"/>
        <font_face value="verdana"/>
        <font_size value="14"/>
        <font_weight value="2"/>
        <font_italic value="0"/>
    </style>
    <style name="posButton">
        <color_back value="cccccc"/>
        <color_fore value="000000"/>
        <font_face value="verdana"/>
        <font_size value="14"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
    </style>
    <style name="buttonDisabled">
        <color_back value="cccccc"/>
        <color_fore value="8e8e8e"/>
        <font_face value="verdana"/>
        <font_size value="14"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
    </style>

    <!-- embedded journal table styles (required but can be customized) -->
    <style name="journalBorder">
        <font_face value="verdana"/>
        <font_size value="0"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
        <color_back value="ffffff"/>
        <color_fore value="ffffff"/>
    </style>
    <style name="journalData">
        <font_face value="verdana"/>
        <font_size value="16"/>
        <font_weight value="0"/>
        <font_italic value="0"/>
        <color_back value="cccccc"/>
        <color_fore value="000000"/>
    </style>
    <style name="journalHeader">
        <font_face value="verdana"/>
        <font_size value="18"/>
        <font_weight value="1"/>
        <font_italic value="0"/>
        <color_back value="cccccc"/>
        <color_fore value="000000"/>
    </style>
    <style name="journalSelected">
        <font_face value="verdana"/>
        <font_size value="16"/>
        <font_weight value="1"/>
        <font_italic value="0"/>
        <color_back value="678FB9"/>
        <color_fore value="000000"/>
    </style>
