<forms>
	<form name="lookupRoutingTask" type="single" 
		target="fieldLookup" title="" default-map-name="lookup"
		default-title-style="tableheadtext" default-widget-style="inputBox" default-tooltip-style="tabletext">
		<auto-fields-service service-name="lookupRoutingTask" map-name="parameters"/>
        <field name="fixedAssetId" >
			<drop-down>
				<list-options list-name="allMachineGroups" list-entry-name="machineGroup" 
								key-name="machineGroup.fixedAssetId" description="${machineGroup.fixedAssetName}"/>
			</drop-down>
        </field>
		<field name="submitButton" title="${uiLabelMap.CommonLookup}" widget-style="smallSubmit"><submit button-type="button"/></field>
		<field name="lookupService"><hidden value="lookupRoutingTask"/></field>
		<field name="formName" ><hidden value="lookupRoutingTask"/></field>
		<field name="id" ><hidden/></field>
	</form>
</forms>