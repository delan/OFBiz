    </request-map>
    <request-map uri="updateReturn">
        <security https="true" auth="true"/>
        <event type="service" invoke="updateReturnHeader"/>
        <response name="success" type="view" value="returnhead"/>
    </request-map>
    <request-map uri="createReturnItems">
