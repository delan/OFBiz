<?xml version="1.0" encoding="UTF-8"?>
<entity-engine-xml>
  <!-- 
    NOTE: All UomConversion conversionFactors are multipliers to change from uomId to uomIdTo.
    In other words there is 1 uomId for conversionFactor uomIdTos, for instance 1 minute for 60 seconds.
  -->

  <!-- =============== TIME_FREQ_MEASURE =============== -->
  <Uom abbreviation="ms" description="Time in Milli-Seconds" uomId="TF_ms" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="s" description="Time in Seconds" uomId="TF_s" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="min" description="Time in Minutes" uomId="TF_min" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="hr" description="Time in Hours" uomId="TF_hr" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="day" description="Time in Days" uomId="TF_day" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="wk" description="Time in Weeks" uomId="TF_wk" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="mon" description="Time in Months" uomId="TF_mon" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="yr" description="Time in Years" uomId="TF_yr" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="" description="Time in Decades" uomId="TF_DECADE" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="" description="Time in Centuries" uomId="TF_CENTURY" uomTypeId="TIME_FREQ_MEASURE"/>
  <Uom abbreviation="" description="Time in Millenia" uomId="TF_MILLENIUM" uomTypeId="TIME_FREQ_MEASURE"/>

  <UomConversion uomId="TF_s" uomIdTo="TF_ms" conversionFactor="1000"/>
  <UomConversion uomId="TF_min" uomIdTo="TF_s" conversionFactor="60"/>
  <UomConversion uomId="TF_hr" uomIdTo="TF_min" conversionFactor="60"/>
  <UomConversion uomId="TF_day" uomIdTo="TF_hr" conversionFactor="24"/>
  <UomConversion uomId="TF_wk" uomIdTo="TF_day" conversionFactor="7"/>
  <UomConversion uomId="TF_yr" uomIdTo="TF_day" conversionFactor="365"/>
  <UomConversion uomId="TF_DECADE" uomIdTo="TF_yr" conversionFactor="10"/>
  <UomConversion uomId="TF_CENTURY" uomIdTo="TF_yr" conversionFactor="100"/>
  <UomConversion uomId="TF_MILLENIUM" uomIdTo="TF_yr" conversionFactor="1000"/>

