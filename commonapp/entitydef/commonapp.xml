    <entity>
      <title>Person Component - Person Entity</title>
      <package-name>org.ofbiz.commonapp.person</package-name>
      <table-name>PERSON</table-name>
      <use-cache>false</use-cache>
      <cmp-field><column-name>USERNAME</column-name><field-type>id-ne</field-type></cmp-field>
      <cmp-field><column-name>PASSWORD</column-name><field-type>id-ne</field-type></cmp-field>
      <cmp-field><column-name>FIRST_NAME</column-name><field-type>name</field-type><validate>isNotEmpty</validate></cmp-field>
      <cmp-field><column-name>MIDDLE_NAME</column-name><field-type>name</field-type></cmp-field>
      <cmp-field><column-name>LAST_NAME</column-name><field-type>name</field-type><validate>isNotEmpty</validate></cmp-field>
      <cmp-field><column-name>TITLE</column-name><field-type>very-short</field-type></cmp-field>
      <cmp-field><column-name>SUFFIX</column-name><field-type>very-short</field-type></cmp-field>
      <cmp-field><column-name>HOME_PHONE</column-name><field-type>tel-number</field-type></cmp-field>
      <cmp-field><column-name>WORK_PHONE</column-name><field-type>tel-number</field-type></cmp-field>
      <cmp-field><column-name>FAX</column-name><field-type>tel-number</field-type></cmp-field>
      <cmp-field><column-name>EMAIL</column-name><field-type>email</field-type><validate>isNotEmpty</validate></cmp-field>
      <cmp-field><column-name>HOME_STREET1</column-name><field-type>long-varchar</field-type></cmp-field>
      <cmp-field><column-name>HOME_STREET2</column-name><field-type>long-varchar</field-type></cmp-field>
      <cmp-field><column-name>HOME_CITY</column-name><field-type>short-varchar</field-type></cmp-field>
      <cmp-field><column-name>HOME_COUNTY</column-name><field-type>short-varchar</field-type></cmp-field>
      <cmp-field><column-name>HOME_STATE</column-name><field-type>short-varchar</field-type></cmp-field>
      <cmp-field><column-name>HOME_COUNTRY</column-name><field-type>short-varchar</field-type></cmp-field>
      <cmp-field><column-name>HOME_POSTAL_CODE</column-name><field-type>short-varchar</field-type></cmp-field>
      <prim-key-column>USERNAME</prim-key-column>
      <finder><column-name>FIRST_NAME</column-name></finder>
      <finder><column-name>LAST_NAME</column-name></finder>
      <finder><column-name>FIRST_NAME</column-name><column-name>LAST_NAME</column-name></finder>
      <finder><column-name>HOME_PHONE</column-name></finder>
      <finder><column-name>EMAIL</column-name></finder>
      <relation>
        <relation-type>many</relation-type>
        <related-table-name>PERSON_PERSON_TYPE</related-table-name>
        <key-map><column-name>USERNAME</column-name></key-map>
        <relation>
          <relation-type>many</relation-type>
          <related-table-name>PERSON_TYPE_ATTRIBUTE</related-table-name>
          <key-map><column-name>TYPE_ID</column-name></key-map>
          <relation>
            <relation-type>one</relation-type>
            <related-table-name>PERSON_ATTRIBUTE</related-table-name>
            <key-map><column-name>USERNAME</column-name></key-map> <!-- this column-name is from the parent in the relation heirarchy, ie PERSON_PERSON_TYPE, and that must be supported -->
            <key-map><column-name>NAME</column-name></key-map>
          </relation>
        </relation>
        <relation>
          <relation-type>one</relation-type>
          <related-table-name>PERSON_TYPE</related-table-name>
          <key-map><column-name>TYPE_ID</column-name></key-map>
        </relation>
      </relation>
      <relation>
        <relation-type>many</relation-type>
        <related-table-name>PERSON_ATTRIBUTE</related-table-name>
        <key-map><column-name>USERNAME</column-name></key-map>
      </relation>
      <relation>
        <relation-type>many</relation-type>
        <related-table-name>PERSON_SECURITY_GROUP</related-table-name>
        <key-map><column-name>USERNAME</column-name></key-map>
        <relation>
          <relation-type>one</relation-type>
          <related-table-name>SECURITY_GROUP</related-table-name>
          <key-map><column-name>GROUP_ID</column-name></key-map>
        </relation>
        <!-- It may be a little too much to display all of the permissions, so just display the groups and let them drill down if they wish
        <relation>
          <relation-type>many</relation-type>
          <related-table-name>SECURITY_GROUP_PERMISSION</related-table-name>
          <key-map><column-name>GROUP_ID</column-name></key-map>
          <relation>
            <relation-type>one</relation-type>
            <related-table-name>SECURITY_PERMISSION</related-table-name>
            <key-map><column-name>PERMISSION_ID</column-name></key-map>
          </relation>
        </relation>
        -->
      </relation>
    </entity>
