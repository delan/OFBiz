<string-processor>
    <string-process field="workEffortId"><copy replace="false"/></string-process>
    <string-process field="workEffortTypeId"><copy/></string-process>
    <string-process field="universalId"><copy/></string-process>
    <string-process field="scopeEnumId"><copy/></string-process>
    <string-process field="currentStatusId"><copy/><not-empty><fail-message message="Status is missing."/></not-empty></string-process>
    <string-process field="priority"><convert type="Long"><fail-message message="Priority is not a valid whole number."/></convert></string-process>
    <string-process field="workEffortName"><copy/><not-empty><fail-message message="Name is missing."/></not-empty></string-process>
    <string-process field="description"><copy/></string-process>
    <string-process field="locationDesc"><copy/></string-process>
    <string-process field="estimatedStartDate"><convert type="Timestamp">
        <fail-message message="Estimated Start Date is not a valid Date-Time."/></convert></string-process>
    <string-process field="estimatedCompletionDate"><convert type="Timestamp">
        <fail-message message="Estimated Completion Date is not a valid Date-Time."/></convert></string-process>
    <string-process field="actualStartDate"><convert type="Timestamp">
        <fail-message message="Actual Start Date is not a valid Date-Time."/></convert></string-process>
    <string-process field="actualCompletionDate"><convert type="Timestamp">
        <fail-message message="Actual Completion Date is not a valid Date-Time."/></convert></string-process>
    <string-process field="estimatedMilliSeconds"><convert type="Double">
        <fail-message message="Estimated Milli-seconds is not a valid number."/></convert></string-process>
    <string-process field="actualMilliSeconds"><convert type="Double">
        <fail-message message="Actual Milli-seconds is not a valid number."/></convert></string-process>
    <string-process field="totalMilliSecondsAllowed"><convert type="Double">
        <fail-message message="Total Milli-seconds Allowed is not a valid number."/></convert></string-process>
    <string-process field="totalMoneyAllowed"><convert type="Double">
        <fail-message message="Total Money Allowed is not a valid number."/></convert></string-process>
    <string-process field="timeTransparency"><convert type="Double">
        <fail-message message="Time Transparency is not a valid whole number."/></convert></string-process>
    <string-process field="moneyUomId"><copy/></string-process>
    <string-process field="specialTerms"><copy/></string-process>
    <string-process field="timeTransparency"><copy/></string-process>
    <string-process field="infoUrl"><copy/></string-process>
</string-processor>
