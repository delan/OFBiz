<jaws>
  <datasource>MainDataSource</datasource>
  <!-- <type-mapping></type-mapping> -->

  <enterprise-beans>
    <entity>
      <ejb-name>UserLogin</ejb-name>
      <table-name>USER_LOGIN</table-name>
        <create-table>true</create-table>
        <remove-table>false</remove-table>
        <tuned-updates>false</tuned-updates>
        <read-only>false</read-only>
        <time-out>300</time-out>

      <cmp-field>
        <field-name>userLoginId</field-name>
        <column-name>USER_LOGIN_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>partyId</field-name>
        <column-name>PARTY_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>contactMechanismId</field-name>
        <column-name>CONTACT_MECHANISM_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>currentUserId</field-name>
        <column-name>CURRENT_USER_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>currentPassword</field-name>
        <column-name>CURRENT_PASSWORD</column-name>
      </cmp-field>

      <finder>
        <name>findByPartyId</name>
        <query>PARTY_ID like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByContactMechanismId</name>
        <query>CONTACT_MECHANISM_ID like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByCurrentUserId</name>
        <query>CURRENT_USER_ID like {0}</query>
        <order></order>
      </finder>
    </entity>
    <entity>
      <ejb-name>LoginAccountHistory</ejb-name>
      <table-name>LOGIN_ACCOUNT_HISTORY</table-name>
        <create-table>true</create-table>
        <remove-table>false</remove-table>
        <tuned-updates>false</tuned-updates>
        <read-only>false</read-only>
        <time-out>300</time-out>

      <cmp-field>
        <field-name>userLoginId</field-name>
        <column-name>USER_LOGIN_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>userLoginSeqId</field-name>
        <column-name>USER_LOGIN_SEQ_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>fromDate</field-name>
        <column-name>FROM_DATE</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>thruDate</field-name>
        <column-name>THRU_DATE</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>partyId</field-name>
        <column-name>PARTY_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>userId</field-name>
        <column-name>USER_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>password</field-name>
        <column-name>PASSWORD</column-name>
      </cmp-field>

      <finder>
        <name>findByUserLoginId</name>
        <query>USER_LOGIN_ID like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByUserId</name>
        <query>USER_ID like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByPartyId</name>
        <query>PARTY_ID like {0}</query>
        <order></order>
      </finder>
    </entity>
    <entity>
      <ejb-name>SecurityGroup</ejb-name>
      <table-name>SECURITY_GROUP</table-name>
        <create-table>true</create-table>
        <remove-table>false</remove-table>
        <tuned-updates>false</tuned-updates>
        <read-only>false</read-only>
        <time-out>300</time-out>

      <cmp-field>
        <field-name>groupId</field-name>
        <column-name>GROUP_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>description</field-name>
        <column-name>DESCRIPTION</column-name>
      </cmp-field>

    </entity>
    <entity>
      <ejb-name>SecurityPermission</ejb-name>
      <table-name>SECURITY_PERMISSION</table-name>
        <create-table>true</create-table>
        <remove-table>false</remove-table>
        <tuned-updates>false</tuned-updates>
        <read-only>false</read-only>
        <time-out>300</time-out>

      <cmp-field>
        <field-name>permissionId</field-name>
        <column-name>PERMISSION_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>description</field-name>
        <column-name>DESCRIPTION</column-name>
      </cmp-field>

      <finder>
        <name>findByPermissionId</name>
        <query>PERMISSION_ID like {0}</query>
        <order></order>
      </finder>
    </entity>
    <entity>
      <ejb-name>SecurityGroupPermission</ejb-name>
      <table-name>SECURITY_GROUP_PERMISSION</table-name>
        <create-table>true</create-table>
        <remove-table>false</remove-table>
        <tuned-updates>false</tuned-updates>
        <read-only>false</read-only>
        <time-out>300</time-out>

      <cmp-field>
        <field-name>groupId</field-name>
        <column-name>GROUP_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>permissionId</field-name>
        <column-name>PERMISSION_ID</column-name>
      </cmp-field>

      <finder>
        <name>findByGroupId</name>
        <query>GROUP_ID like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByPermissionId</name>
        <query>PERMISSION_ID like {0}</query>
        <order></order>
      </finder>
    </entity>
    <entity>
      <ejb-name>UserLoginSecurityGroup</ejb-name>
      <table-name>USER_LOGIN_SECURITY_GROUP</table-name>
        <create-table>true</create-table>
        <remove-table>false</remove-table>
        <tuned-updates>false</tuned-updates>
        <read-only>false</read-only>
        <time-out>300</time-out>

      <cmp-field>
        <field-name>userLoginId</field-name>
        <column-name>USER_LOGIN_ID</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>groupId</field-name>
        <column-name>GROUP_ID</column-name>
      </cmp-field>

      <finder>
        <name>findByUserLoginId</name>
        <query>USER_LOGIN_ID like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByGroupId</name>
        <query>GROUP_ID like {0}</query>
        <order></order>
      </finder>
    </entity>
  </enterprise-beans>
</jaws>

