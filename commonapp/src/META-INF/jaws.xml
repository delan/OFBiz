      </cmp-field>
      <cmp-field>
        <field-name>homePostalCode</field-name>
        <column-name>HOME_POSTAL_CODE</column-name>
      </cmp-field>

      <finder>
        <name>findByFirstName</name>
        <query>FIRST_NAME like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByLastName</name>
        <query>LAST_NAME like {0}</query>
        <order></order>
      </finder>
      <finder>
        <name>findByFirstNameAndLastName</name>
        <query>FIRST_NAME like {0} AND LAST_NAME like {1}</query>
        <order></order>
      </finder>
      <finder>
