        <query>,SECURITY_GROUP_PERMISSION
        WHERE PERSON_SECURITY_GROUP.GROUP_ID=SECURITY_GROUP_PERMISSION.GROUP_ID AND USERNAME={0} AND SECURITY_GROUP_PERMISSION.PERMISSION_ID={1}</query>
        <order>GROUP_ID</order>
      </finder>
      <finder>
        <name>findByUsername</name>
        <query>USERNAME like {0}</query>
