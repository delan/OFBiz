    <config>
        <description>sample: remove jsessionid</description>
        <seo>
            <url-pattern>^(.*);jsessionid=(.*)jvm[1-9](.*)$</url-pattern>
            <replacement>$1$3</replacement>
        </seo>
    </config>
    
</urlregexp>
